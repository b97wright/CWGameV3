{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build CWGameV3 Development Win64",
            "group": "build",
            "type": "shell",
            "command": "\"${workspaceFolder}\\CWGameV3.uproject\" -run=compile -game -progress",
            "problemMatcher": [
                "$msCompile"
            ],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            }
        },
        {
            "label": "Build CWGameV3 (Batch File)",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "type": "shell",
            "command": "${workspaceFolder}\\.vscode\\build.bat",
            "problemMatcher": [
                "$msCompile"
            ],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            }
        }
    ]
} 